<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/vidyaastra/supply-chain#"
     xml:base="http://www.semanticweb.org/vidyaastra/supply-chain"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/vidyaastra/supply-chain"/>

    <!-- Properties -->
    <owl:ObjectProperty rdf:about="#suppliesComponent">
        <rdfs:domain rdf:resource="#Supplier"/>
        <rdfs:range rdf:resource="#Component"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#requiresComponent">
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="#Component"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#locatedIn">
        <rdfs:domain rdf:resource="#Supplier"/>
        <rdfs:range rdf:resource="#Location"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#revenueImpact">
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#leadTimeDays">
        <rdfs:domain rdf:resource="#Component"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <!-- Classes -->
    <owl:Class rdf:about="#Supplier"/>
    <owl:Class rdf:about="#Component"/>
    <owl:Class rdf:about="#Product"/>
    <owl:Class rdf:about="#Location"/>

    <!-- Individuals -->
    <owl:NamedIndividual rdf:about="#Taiwan">
        <rdf:type rdf:resource="#Location"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#TSMC">
        <rdf:type rdf:resource="#Supplier"/>
        <locatedIn rdf:resource="#Taiwan"/>
        <suppliesComponent rdf:resource="#A16_Chip"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#A16_Chip">
        <rdf:type rdf:resource="#Component"/>
        <leadTimeDays rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">90</leadTimeDays>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#iPhone_16">
        <rdf:type rdf:resource="#Product"/>
        <requiresComponent rdf:resource="#A16_Chip"/>
        <revenueImpact rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">50000000.00</revenueImpact>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#MacBook_Pro">
        <rdf:type rdf:resource="#Product"/>
        <requiresComponent rdf:resource="#A16_Chip"/>
        <revenueImpact rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">25000000.00</revenueImpact>
    </owl:NamedIndividual>
</rdf:RDF>
