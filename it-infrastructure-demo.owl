<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/vidyaastra/it-infra#"
     xml:base="http://www.semanticweb.org/vidyaastra/it-infra"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/vidyaastra/it-infra"/>

    <!-- Properties -->
    <owl:ObjectProperty rdf:about="#hostsService">
        <rdfs:domain rdf:resource="#Server"/>
        <rdfs:range rdf:resource="#Service"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#dependsOn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="#Service"/>
        <rdfs:range rdf:resource="#Service"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#status">
        <rdfs:domain rdf:resource="#Server"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#criticality">
        <rdfs:domain rdf:resource="#Service"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Classes -->
    <owl:Class rdf:about="#Server"/>
    <owl:Class rdf:about="#Service"/>
    <owl:Class rdf:about="#Database"/>

    <!-- Individuals -->
    <owl:NamedIndividual rdf:about="#Server_DB_01">
        <rdf:type rdf:resource="#Server"/>
        <status>OFFLINE</status>
        <hostsService rdf:resource="#User_DB"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#User_DB">
        <rdf:type rdf:resource="#Service"/>
        <criticality>HIGH</criticality>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Auth_Service">
        <rdf:type rdf:resource="#Service"/>
        <dependsOn rdf:resource="#User_DB"/>
        <criticality>CRITICAL</criticality>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Web_Portal">
        <rdf:type rdf:resource="#Service"/>
        <dependsOn rdf:resource="#Auth_Service"/>
        <criticality>MEDIUM</criticality>
    </owl:NamedIndividual>
</rdf:RDF>
